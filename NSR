#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;cin>>t;
    while(t--){
        int n;cin>>n;
        stack<int> s;
        int a[n];for(int i=0;i<n;i++)cin>>a[i];
        int nsr[n];
        for(int i=n-1;i>=0;i--)
        {
            if(s.size()==0){nsr[i]=-1;;}
            else if(s.size()!=0&&a[i]<=s.top())
            {
                while(s.size()>0&&a[i]<=s.top())s.pop();
                if(s.size()!=0)a[i]=s.top();else nsr[i]=-1;
              
            }
            else  {
                nsr[i]=s.top();
               
                
            }s.push(a[i]);
            
        }
        for(int i=0;i<n;i++)
        {
            cout<<nsr[i]<<" ";
            
        }
        
        
        
    }
}
